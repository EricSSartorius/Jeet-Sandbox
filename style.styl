@import 'typographic'
@import 'styl/*' 
@import 'nib'
@import 'jeet'

normalize-base()
normalize-html5()
edit()

t-presets(1)

marg(a)	
	return a a
	
rupture.scale = 0 400px 600px 800px 1050px 1800px
rupture.scale-names = 'xs' 'sm' 'md' 'lg' 'xl' 'hd'
 

section
	center(960px)
aside
	col(1/3)
	shift(2/3)
	+below(sm)
		stack()
		unshift()
article
	col(2/3)
	shift(-1/3)
	+below(sm)
		stack()
		unshift()
	img
		max-width 100%

